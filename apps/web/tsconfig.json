{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"paths": {
			"$lib/*": ["src/lib/*"],
			"$app/*": ["./.svelte-kit/types/src/app/*"]
		}
	},
	"include": [
		"src",
		"src/**/*.ts",
		"src/**/*.svelte",
		"src/**/*.spec.ts",
		"src/**/*.test.ts",
		"e2e/**/*.ts"
	],
	"references": [
		{ "path": "../../packages/domain" },
		{ "path": "../../packages/pdf-engine" },
		{ "path": "../../packages/shared" }
	]
}
